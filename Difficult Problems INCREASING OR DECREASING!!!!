 int difProblem(string N) {
      for(int i=1;i<N.size()-1;i++){
          if(N[i+1]<N[i] && N[i]>N[i-1]){
              return 0;
              break;
          }
      }
       for(int i=1;i<N.size()-1;i++){
          if(N[i+1]>N[i] && N[i]<N[i-1]){
              return 0;
              break;
          }
      }
      
      return 1;}
